{
  "name": "data-sets",
  "version": "1.0.0",
  "description": "Data sets for testing with realistic but synthetic data",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf lib && tsc -p tsconfig.build.json",
    "docs": "typedoc && touch docs/.nojekyll",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "pretest": "npm run build",
    "test": "jest && pkg-ok",
    "watch": "tsc -w -p tsconfig.build.json"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "faker",
    "data",
    "mock",
    "fast-check"
  ],
  "author": "Daniel Bartholomae <daniel@bartholomae.name> (https://startup-cto.net)",
  "funding": "https://github.com/sponsors/dbartholomae",
  "license": "MIT",
  "homepage": "https://dbartholomae.github.io/data-sets",
  "repository": "git@github.com:dbartholomae/data-sets.git",
  "bugs": "https://github.com/dbartholomae/data-sets/issues",
  "directories": {
    "lib": "lib",
    "doc": "docs"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^8.0.0",
    "@types/jest": "^27.0.0",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "fast-check": "^2.20.0",
    "husky": "^7.0.0",
    "jest": "^27.0.1",
    "lint-staged": "^12.0.2",
    "pkg-ok": "^2.3.1",
    "prettier": "^2.1.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^18.0.0",
    "ts-jest": "^27.0.1",
    "tslib": "^2.0.1",
    "typedoc": "^0.22.9",
    "typescript": "^4.0.2"
  }
}
